<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="assignment7.view.Login">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="400.0">
         <top>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Select Chat Type" BorderPane.alignment="BOTTOM_CENTER">
               <BorderPane.margin>
                  <Insets top="50.0" />
               </BorderPane.margin>
            </Text>
         </top>
         <center>
            <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="group" mnemonicParsing="false" onAction="#handleButtG" text="Group">
                     <HBox.margin>
                        <Insets left="50.0" top="100.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="priv" mnemonicParsing="false" onAction="#handleButtP" text="Private">
                     <HBox.margin>
                        <Insets left="200.0" top="100.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
